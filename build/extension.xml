<extension xmlns="http://ns.adobe.com/air/extension/16.0"> 
    <id>com.trembit.utils</id> 
    <versionNumber>0.0.1</versionNumber> 
    <platforms> 
    	<platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libAudioUtilsExtension.jar</nativeLibrary>
                <initializer>com.trembit.utils.AudioUtilsExtension</initializer>
            </applicationDeployment>
        </platform>

		<platform name="iPhone-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>libAudioUtilsExtension.a</nativeLibrary> 
                <initializer>AudioUtilsExtensionInitializer</initializer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms> 
</extension>