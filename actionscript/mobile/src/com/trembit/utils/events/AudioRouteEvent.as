package com.trembit.utils.events{	import flash.events.Event;		public class AudioRouteEvent extends Event	{		/**		 * Disatched when the audio route on the decive changes. 		 */				public static const AUDIO_ROUTE_CHANGED:String = "audioRouteChanged";				public var route : String;				public function AudioRouteEvent( type:String, route : String = "", bubbles:Boolean=false, cancelable:Boolean=false ) {			this.route = route;						super(type, bubbles, cancelable);		}	}}